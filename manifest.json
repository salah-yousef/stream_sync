{
  "manifest_version": 2,
  "name": "stream together- Host!",
  "version": "0.1",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.netflix.com/*",
        "https://*.netflix.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "ct.png"
  }
}